    <title>Dashboard</title>
    <script src="https://code.jquery.com/jquery-3.6.0.slim.js" integrity="sha256-HwWONEZrpuoh951cQD1ov2HUK5zA5DwJ1DNUXaM6FsY=" crossorigin="anonymous"></script>

 
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">



 
    <!-- Custom styles for this template -->
    <link href="css/dashboard.css" rel="stylesheet">

    <style>
      .button {
    font-family: sans-serif;
    font-size: 0.9rem;
    background-color: #626262;
    color: white;
    padding: 0.40rem 1.5rem;
    border-radius: 0.25rem;
    text-decoration: none;
    box-shadow: 0px 10px 36px rgba(0, 0, 0, 0.15);
}

.button:hover {
    background-color: rgb(35, 35, 248);
    color: white;
}

.label {
    text-decoration: none;
    letter-spacing: 3px;
    font-size: 1rem;  
    font-weight: 400; 
    color: #626262;
    margin: 10px 0px 5px 0px;
}
    </style>

  </head>
  <body>
    
<header class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
  <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" href="#">Dashboard</a>
  <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="navbar-nav">
    <div class="nav-item text-nowrap">
      <a class="nav-link px-3" href="/">Sign out</a>
    </div>
  </div>
</header>

<div class="container-fluid" style="z-index: 1;">
  <div class="row">
    <nav id="sidebarMenu" style="z-index: 0;" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link active" id="home" aria-current="page" href="#">
              <span data-feather="home"></span>
              Schedule Meeting
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#" id="list">
              <span data-feather="file"></span>
              List of Applicants
            </a>
          </li>
         
        </ul>

        <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
          <span>Account</span>
          <a class="link-secondary" href="#" aria-label="Add a new report">
            <span data-feather="plus-circle"></span>
          </a>
        </h6>
        <ul class="nav flex-column mb-2">
          <li class="nav-item">
            <a id="account" class="nav-link" href="#">
              <span data-feather="file-text"></span>
              Account Details
            </a>
          </li>
        </ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      

      <div id="home-content" style="margin-top: 30px;">
        <div class="row card" style="margin: 0px 10px 10px 10px; padding: 20px 20px 40px 20px;">
    
            <div class="col-lg-12">
    
    
                
              <form action="/compose" method="post">
                    <div class="form-group">
    
                        <div class="row">
                            <input  style="display: none;"  type="text" class="form-control"
                            name="userName" value="<%=userName%>">
    
                            <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label"> Title</label>
                                <br>
                                <textarea type="text" class="form-control" name="meetingTitle" cols="2" rows="2"
                                    required></textarea>
                            </div>
    
    
    
                            <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label"> Agenda </label>
                                <textarea type="text" class="form-control" name="meetingAgenda" cols="20" rows="2"
                                    required></textarea>
                            </div>
    
                            <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label"> Date </label>
                                    <input type="date" class="form-control" placeholder="Ex- 05:00pm 2/01/22"
                                    name="meetingdate">
    
                            </div>
    
                           
                            <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label">Via</label>
                                <div class="input-group mb-3">
                                    <select class="form-select" name="meetingType" id="inputGroupSelect02">
                                        <option selected>Choose...</option>
                                        <option value="gmeet">Google Meet</option>
                                        <option value="zoom">Zoom</option>
                                    </select>
                                </div>
                            </div>
    
    
                            <div class="col-lg-12 col-lg-12 col-sm-12" style="margin: 20px 0px 0px 0px;">
    
                                <h5>Host Details</h5>
                            </div>
    
    
                            <div class="col-lg-6 col-lg-6 col-sm-12"> <label class="label">Name </label>
                                <input type="text" class="form-control" placeholder="Full Name"
                                    name="organisationName">
    
                            </div>
                            <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label">Email </label>
                                <input type="text" class="form-control" placeholder="Email"
                                    name="organisationEmail">
                            </div>
    
                            <div class="col-lg-6 col-lg-6 col-sm-12">
    
                                <label class="label">Number </label>
    
                                <input type="text" class="form-control" placeholder="Contact Number"
                                    name="organisationNumber">
                            </div>
                           
                            <div class="col-lg-6 col-lg-6 col-sm-12">
    
                                
                            </div>
    
                        </div>
    
    
                    </div>
                    <br>
                    <button  class="btn button" type="submit" name="button">Publish</button>


                    
              </form>
            </div>
        </div>
        </div>
        <div  id="list-content"  class="row card" style="margin: 30px 0px 10px 0px; padding: 0px 10px 40px 10px; ">

          <table class="table table-striped table-hover" style="margin-top: 20px; padding: 0px 0px;">
        
      
              <tbody>
                <th>Name</th>
                <th>Email</th>
                <th>Conference Title</th>
                <% for(var i=(users.length)-1; i>=0; i--){ %>
                <tr>
                  <td><%=users[i].userName%></td>
                  <td><%=users[i].userEmail%></td>
                  <td><%=users[i].userMeetName%></td>
                </tr>
                <% } %>
              </tbody>
            </table>
          </div>
      
        <div id="account-content" style="margin-top: 30px;">
          <div class="row card" style="margin: 0px 10px 10px 10px; padding: 20px 20px 20px 20px;">
      
              <div class="col-lg-12">
      
      
                <% for(var i=(details.length)-1; i>=0; i--){ %>
                  
             
      
                          <div class="row">
                    
      
                              <div class="col-lg-6 col-lg-6 col-sm-12">
                              <label class="label">Name: </label>
                              <td><%=details[i].fname%></td>
                              <td><%=details[i].lname%></td>

                              
                               
                              </div>
                              <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label">Email: </label>
                            <td><%=details[i].email%></td>
                             
                            </div>
      
      
                              <div class="col-lg-6 col-lg-6 col-sm-12">
                              <label class="label"> Contact: </label>
                              <td><%=details[i].contact%></td>, 
                              <td><%=details[i].contact2%></td>
                              </div>

                             
      
                              <div class="col-lg-6 col-lg-6 col-sm-12">
                                  <label class="label"> College: </label>
                              <td><%=details[i].college%></td>
                                  
                              </div>
      
                           
      
      
                           
                             

                              <div class="col-lg-6 col-lg-6 col-sm-12">
                                <label class="label">Address: </label>
                            <td><%=details[i].address%></td>, 
                            <td><%=details[i].state%></td>, 
                            <td><%=details[i].country%></td>.
                             
                            </div>
      
                            
                             
                              <div class="col-lg-6 col-lg-6 col-sm-12">
      
                                  
                              </div>
      
                          </div>
      
      
                      </div>
                      <br>
                      <% } %>
  
  
                      
              
          </div>
          </div>

    
    </main>
  </div>
</div>

<script>
  $(document).ready(function(){
				$('#account-content').hide();
		    $('#list-content').hide();

    $('#home').click(function(){
				$('#account-content').hide();
        $('#list-content').hide();
				$('#home-content').show();
        $('#home').addClass('active');
        $('#list').removeClass('active');
        $('#account').removeClass('active');
			});

      $('#list').click(function(){
				$('#home-content').hide();
				$('#account-content').hide();
				$('#list-content').show();
        $('#list').addClass('active');
        $('#home').removeClass('active');
        $('#account').removeClass('active');
			});

      
    $('#account').click(function(){
        $('#list-content').hide();
				$('#home-content').hide();
				$('#account-content').show();
        $('#account').addClass('active');
        $('#home').removeClass('active');
        $('#list').removeClass('active');
			});
  });
</script>


